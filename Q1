// Write a program to input two numbers and display their sum

#include<stdio.h>
int main()
{
    // INPUT TWO NUMBER AND DO THEIR SUM
    int x,y, sum;
    printf("Enter first number:");
    scanf("%d",&x);
    printf("enter second number");
    scanf("%d", &y);
    sum = x+y;
    printf("The sum of %d and %d is : %d\n",x,y,sum);
    return 0;
}

    
